<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:height="2dp"
        android:gravity="center"
        android:left="30dp"
        android:right="30dp">
        <shape>
            <solid android:color="@color/light_grey" />
        </shape>
    </item>

    <item
        android:width="2dp"
        android:bottom="30dp"
        android:gravity="center"
        android:top="30dp">
        <shape>
            <solid android:color="@color/light_grey" />
        </shape>
    </item>

    <item
        android:width="150dp"
        android:height="150dp"
        android:bottom="10dp"
        android:gravity="center"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape>
            <corners android:radius="4dp" />
            <stroke
                android:width="1dp"
                android:color="@color/light_grey" />
        </shape>
    </item>
</layer-list>